{
    "name": "airylinus/planka-php-sdk",
    "type": "library",
    "description": "An easy-to-use PHP Class for accessing Planka's API. Fork from decole/planka-php-sdk",
    "keywords": ["planka", "api"],
    "homepage": "https://github.com/airylinus/planka-php-sdk",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Mongo Hsiung",
            "email": "airy@live.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "symfony/http-client": "^6.2",
        "symfony/mime": "^6.2",
        "fp4php/functional": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Planka\\Bridge\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.1.3",
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "vimeo/psalm": "^5.11"
    }
}
