{
    "name": "decole/planka-php-sdk",
    "type": "library",
    "description": "An easy-to-use PHP Class for accessing Planka's API.",
    "keywords": ["planka", "api"],
    "homepage": "https://github.com/decole/planka-php-sdk",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Galochkin Sergey",
            "email": "decole@rambler.ru"
        },
        {
            "name": "Airy",
            "email": "airy@live.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.3.2",
    "require": {
        "php": ">=8.1.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/mime": "^6.4|^7.0",
        "fp4php/functional": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Planka\\Bridge\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "scripts": {
        "fix-cs": [
            "@php ./vendor/bin/php-cs-fixer fix --diff -v --allow-risky=yes --ansi"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "vimeo/psalm": "^5.11",
        "friendsofphp/php-cs-fixer": "^3.65"
    }
}
